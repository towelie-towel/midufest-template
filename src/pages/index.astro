---
import Layout from '@layouts/Layout.astro'
import Landing from '@components/sections/Landing.astro'
import CodigoDeConducta from '@components/sections/CodigoDeConducta.astro'
import Ticket from '@components/sections/Ticket.astro'
import Speakers from '@components/sections/Speakers.astro'
import Agenda from '@components/sections/Agenda.astro'
import Sponsors from '@components/sections/Sponsors.astro'

const ticketUserName = Astro.url.searchParams.get('ticket') ?? '';
---

<Layout title='midufest - ¡La fiesta de la programación!' ticketUserName={ticketUserName}>
	<Landing />
	<Sponsors />
	<Speakers />
	<Agenda />
	<Ticket />
	<CodigoDeConducta />
</Layout>

